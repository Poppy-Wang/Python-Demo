package lesson1;

public class demo2 {
	public static void main(String[] args) {
	//定义boolean类型变量,只能true或者false
	boolean aa=true;
	boolean bb=false;
	//整型:byte类型最大为127
	byte cc=127;
	short dd=3;
	int ee=34;
	long ff=123;
	//浮点型：float,double,小数默认会被提升为double类型，所以定义float类型需要加后缀f,float类型占4个字节，
	//double类型占8个字节，大类型转换成小类型必须要强制转换，此过程需要手动强制转换
	float a=3.14f;
	double b=3.4;
	//字符型char
	char c='12';
}
}
